<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="?dividerVerticalSize" android:color="?dividerVerticalColor" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:top="?dividerVerticalSize" android:bottom="?dividerVerticalSize">
        <shape android:shape="rectangle">
            <stroke android:width="?dividerVerticalSize" android:color="@color/white" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>